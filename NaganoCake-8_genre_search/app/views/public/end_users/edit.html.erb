<h4>会員情報編集</h4>
<%= form_with model:@user,url: public_end_users_path, local: true do |f| %>
 <div class="field">
   <p>氏名</p>
   <%= f.text_field :last_name %>
   <%= f.text_field :first_name %>
 </div>
 <div class="field">
   <p>フリガナ</p>
   <%= f.text_field :last_kana_name %>
   <%= f.text_field :first_kana_name %>
 </div>
 <div class="field">
   <p>郵便番号</p>
   <%= f.text_field :postal_code %>
 </div>
  <div class="field">
   <p>住所</p>
   <%= f.text_area :address %>
 </div>
  <div class="field">
   <p>電話番号</p>
   <%= f.text_field :telephone_number %>
 </div>
  <div class="field">
   <p>メールアドレス</p>
   <%= f.email_field :email %>
 </div>
 
  <%= f.submit '編集内容を保存' %>
<% end %>
<%= link_to "退会する", public_quit_end_users_path, class: "btn btn-secondary w-120px mt-2"  %>